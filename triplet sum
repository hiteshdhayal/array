 //Your Code Here
        sort(a,a+n);
        int start=0;
        int end=n-1;
        for(int i=i+1;i<n-2;i++){
            int value=a[i];
        
        while(start<end){
            if(start+end+value==x)
                return true;
              else if(start+end+value>x)
                    end--;
              else 
                    start++;
              }
            
        }
       return false;
    }
    
    
